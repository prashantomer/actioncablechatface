%h2= link_to "Logout", destroy_user_session_path, method: :delete
.col-md-12.col-sm-12
  %ul#conversations-list
    - @conversations.each do |conversation|
      = render 'conversations/conversation', conversation: conversation, user: current_user
%div{:style => "clear:both"}
.col-md-12.col-sm-12
  .panel.panel-primary
    .panel-heading
      %h3.panel-title User list
    .panel-body
      %ul
        - @users.each do |user|
          %li= link_to user.email, conversations_path(user_id: user), remote: true, method: :post
